<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="student" class="com.example.demo.StudentBean" init-method="init" destroy-method="destroybean">
<property name="name" value="priya"></property>
<property name="address" ref="address"></property>
</bean>


<bean id="address" class="com.example.demo.Address">
<property name="street" value="alandi"></property>
<property name="city" value="pune"></property>
<property name="state" value="maharashtra"></property>
</bean>


</beans>